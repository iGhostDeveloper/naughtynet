            }
            
            .video-player {
                height: 250px;
            }
            
            .play-button {
                width: 60px;
                height: 60px;
            }
            
            .cta-button {
                padding: 15px 30px;
                font-size: 1.1rem;
            }
        }
    </style>
</head>
