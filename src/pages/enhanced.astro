        }

        .stat-number {
            font-size: 1.5rem;
            font-weight: bold;
            color: #ff6b6b;
            display: block;
        }

        .stat-label {
            font-size: 0.9rem;
            color: #cccccc;
        }

        @keyframes pulse-glow {
